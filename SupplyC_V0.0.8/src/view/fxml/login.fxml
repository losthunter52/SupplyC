<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/17"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.view.loginController">
	<children>
		<Pane prefHeight="400.0" prefWidth="182.0"
			style="-fx-background-color: #08223C;">
			<children>
				<Label layoutX="148.0" layoutY="14.0" text="S"
					textFill="WHITE">
					<font>
						<Font name="Impact" size="66.0" />
					</font>
				</Label>
			</children>
		</Pane>
		<TextField fx:id="txtLogin" layoutX="336.0" layoutY="219.0"
			onKeyPressed="#txtLoginKeyPressed" prefHeight="26.0"
			prefWidth="200.0" />
		<PasswordField fx:id="txtSenha" layoutX="336.0"
			layoutY="276.0" onKeyPressed="#txtSenhaKeyPressed" prefHeight="26.0"
			prefWidth="200.0" />
		<Label layoutX="336.0" layoutY="199.0" text="Login" />
		<Label layoutX="336.0" layoutY="258.0" text="Senha" />
		<Button fx:id="btnLogin" layoutX="489.0" layoutY="311.0"
			mnemonicParsing="false" onAction="#btnLoginClicked"
			onKeyPressed="#btnEntrarKeyPressed" text="Entrar" />
		<Label layoutX="283.0" layoutY="102.0" text="Bem Vindo!"
			textFill="#000000a6">
			<font>
				<Font size="50.0" />
			</font>
		</Label>
		<Label layoutX="344.0" layoutY="167.0"
			text="Entre Com Sua Identificação Abaixo:" textFill="#000000a7" />
		<Label layoutX="183.0" layoutY="15.0" text="upplyC"
			textFill="#08223c">
			<font>
				<Font name="Impact" size="64.0" />
			</font>
		</Label>
	</children>
</AnchorPane>
